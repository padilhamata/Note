ancel = () => {\n    this.setState({ isEditing: false });\n  };\n\n  handleSave = () => {\n    this.props.onEdit(this.props.note.id, this.input.value);\n    this.setState({ isEditing: false });\n  };\n\n  render() {\n    const { note, onEdit, onDelete, index, onMove, total } = this.props;\n    const { isEditing } = this.state;\n    return (\n      <div className=\"note\">\n        {isEditing ? (\n          <input\n            type=\"text\"\n            className=\"note_input\"\n            defaultValue={note.text}\n            ref={c => {\n              this.input = c;\n            }}\n          />\n        ) : (\n          <span className=\"note_text\">{note.text}</span>\n        )}\n        {isEditing && (\n          <React.Fragment>\n            <button\n              className=\"note_button note_button--red\"\n              onClick={this.handleCancel}\n            >\n              <i class=\"material-icons\">cancel</i>\n            </button>\n            <button\n              className=\"note_button note_button--green\"\n              onClick={this.handleSave}\n            >\n              <i class=\"material-icons\">done_outline</i>\n            </button>\n          </React.Fragment>\n        )}\n        <button\n          disabled={isEditing}\n          className=\"note_button\"\n          onClick={this.handleEdit}\n        >\n          <i class=\"material-icons\">edit</i>\n        </button>\n        <button\n          disabled={isEditing}\n          className=\"note_button\"\n          onClick={() => {\n            onDelete(note.id);\n          }}\n        >\n          <i class=\"material-icons\">delete</i>\n        </button>\n\n        <button\n          className={className(\"note_button\", {\n            \"note_button--hidden\": index === 0\n          })}\n          onClick={() => {\n            onMove(\"up\", index);\n          }}\n        >\n          <i class=\"material-icons\">arrow_upward</i>\n        </button>\n\n        <button\n          className={className(\"note_button\", {\n            \"note_button--hidden\": index === total - 1\n          })}\n          onClick={() => {\n            onMove(\"down\", index);\n          }}\n        >\n          <i class=\"material-icons\">arrow_downward</i>\n        </button>\n      </div>\n    );\n  }\n}\n\nexport default Note;\n"},"lineCount":null}},"error":null,"hash":"8dd754f27f8eb5356b749c8280751025","cacheData":{"env":{}}}