  } @else if $level == 5 {\n    box-shadow: 0 19px 38px rgba(0, 0, 0, 0.3), 0 15px 12px rgba(0, 0, 0, 0.22);\n  }\n}\n\n@-webkit-keyframes rotation {\n  from {\n    -webkit-transform: rotate(0deg);\n  }\n  to {\n    -webkit-transform: rotate(359deg);\n  }\n}\n\n@mixin rotation() {\n  animation: rotation 2s infinite linear;\n}\n","index.scss":"@import \"mixin\";\n\n$app-padding: 20px;\n$container-max-width: 600px;\n$color-primary: #03a9f4;\n$color-danger: #f44336;\n$color-sucess: #4caf50;\n\nbody {\n  background-color: #e8e8e8;\n  margin: 0;\n  padding: 0;\n  font-family: \"Roboto\", sans-serif;\n  color: #212121;\n}\n\n.about {\n  text-align: center;\n}\n\n.error {\n  text-align: center;\n\n  &_button {\n    background-color: $color-primary;\n    border: none;\n    border-radius: 2px;\n    padding: 10px 12px;\n    cursor: pointer;\n    color: #fff;\n  }\n}\n\n.navigation-drawer {\n  position: fixed;\n  top: 0;\n  left: -280px;\n  height: 100vh;\n  width: 260px;\n  background-color: #fff;\n  @include box-shadow(5);\n  transition: left 0.5s ease-in-out;\n\n  &--open {\n    left: 0;\n  }\n  &_head {\n    height: 46px;\n    display: flex;\n    align-items: center;\n    justify-content: flex-end;\n    padding-right: 12px;\n\n    &_button {\n      background-color: transparent;\n      border: none;\n      cursor: pointer;\n    }\n  }\n\n  &_menu {\n    margin-top: 16px;\n    padding: 0 8px;\n\n    &_item {\n      display: block;\n      width: 100%;\n      background-color: transparent;\n      border: none;\n      display: flex;\n      align-items: center;\n      margin-bottom: 12px;\n      cursor: pointer;\n      padding: 6px 8px;\n      i {\n        margin-right: 8px;\n      }\n\n      &:hover {\n        background-color: rgba(0, 0, 0, 0.08);\n      }\n    }\n  }\n}\n\n.app-bar {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  background-color: $color-primary;\n  color: #fff;\n  @include box-shadow(2);\n\n  &_container {\n    height: 46px;\n    display: flex;\n    align-items: center;\n    margin: 0 auto;\n    padding: 0 $app-padding;\n    max-width: $container-max-width;\n  }\n  &_brand {\n    flex: 1;\n    padding-left: 8px;\n  }\n\n  &_action {\n    background-color: transparent;\n    border: none;\n    color: #fff;\n    border-radius: 2px;\n    cursor: pointer;\n\n    &:focus {\n      outline: none;\n    }\n\n    &:hover {\n      background-color: rgba(0, 0, 0, 0.08);\n    }\n    &--rotation {\n      @include rotation();\n    }\n\n    &--danger {\n      color: $color-danger;\n    }\n  }\n}\n\n.container {\n  margin: 0 auto;\n  padding: 70px $app-padding;\n  max-width: $container-max-width;\n}\n.new-note {\n  background-color: #fff;\n  padding: 10px 12px;\n  border-radius: 2px;\n  @include box-shadow(1);\n  &_input {\n    width: 100%;\n    border: none;\n\n    &:focus {\n      outline: none;\n    }\n  }\n}\n\n.note-list {\n  margin: 40px 0;\n}\n\n.note {\n  display: flex;\n  align-items: center;\n  margin-bottom: 20px;\n  background-color: #fff;\n  padding: 10px 12px;\n  border-radius: 2px;\n  @include box-shadow(1);\n\n  &_text {\n    flex: 1;\n  }\n\n  &_input {\n    flex: 1;\n    border-top: 0;\n    border-right: 0;\n    border-left: 0;\n    border-bottom: 1px solid #c7c7c7;\n  }\n\n  &_button {\n    background-color: transparent;\n    border: 0;\n    cursor: pointer;\n\n    &--hidden {\n      visibility: hidden;\n    }\n\n    &--red {\n      color: $color-danger;\n    }\n\n    &--green {\n      color: $color-sucess;\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"ba75347c182bbd2dbf883feb0cdfaa5e","cacheData":{"env":{}}}